ed)\n      switch (passed) {\n        case 'firstName':\n          this.lastName.focus();\n          break;\n        case 'lastName':\n          this.age.focus();\n          break;\n        case 'age':\n          this.submit.focus();\n          break;\n        default:\n          this.submit.focus();\n      }\n    }\n  }\n  onSubmit = () => {\n    alert(`yey submited! ${this.firstName.value}, ${this.lastName.value} `);\n  }\n  render() {\n    return (\n      <div className=\"App\">\n\n        <div>\n          <FuncCustomComp/>\n        </div>\n\n        <div>\n          <span>First Name: </span>\n          <input\n            type=\"text\"\n            onKeyUp={this.onKeyUp.bind(this, 'firstName')}\n            ref={(input) => { this.firstName = input; }} />\n        </div>\n        <div>\n          <span>Last Name: </span>\n          <input\n            type=\"text\"\n            onKeyUp={this.onKeyUp.bind(this, 'lastName')}\n            ref={(input) => { this.lastName = input; }} />\n        </div>\n        <div>\n          <span>Age: </span>\n          <input\n            type=\"text\"\n            onKeyUp={this.onKeyUp.bind(this, 'age')}\n            ref={(input) => { this.age = input; }} />\n        </div>\n        <div>\n        <input\n          type=\"submit\"\n          value=\"Submit\"\n          onClick={this.onSubmit}\n          ref={(input) => { this.submit = input; }}\n        />\n         </div>\n      </div>\n    );\n  }\n}\nconst styles = {\n  myInput : {\n    'margin' : '20px'\n  }\n}\n\nexport default FormProf;\n","/Users/fred/FRED_PERSO/développement/react/my-beautiful-react-app/src/components/Member.js",[],"/Users/fred/FRED_PERSO/développement/react/my-beautiful-react-app/src/components/ButtonOne.js",[],"/Users/fred/FRED_PERSO/développement/react/my-beautiful-react-app/src/components/parentToChild/Parent.js",[],"/Users/fred/FRED_PERSO/développement/react/my-beautiful-react-app/src/components/ChildX.js",["74"],"import React, { Component } from 'r